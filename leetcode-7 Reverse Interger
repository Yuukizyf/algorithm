class Solution {
public:
    int reverse(int x) {
        int re_x = 0;

        while(x!=0){
            int tmp = re_x*10+x%10;//reverse
            x = x/10;              //右移一位
            if(tmp/10!=re_x){      //判断是否正确
                re_x = 0;
                break;
            }
            re_x = tmp;
        }
        return re_x;
        
    }
};
